    .title {
        color: #69bc1f;
        font-size: 2em;
        width: 100%;
        padding-bottom:24px;
    }
    
    .bold {
        font-weight: bold;
        line-height: 2em;
        color: rgba(0, 0, 0, 0.54);
    }

    .message {
        line-height: 2em;
        color: rgba(0, 0, 0, 0.54);
    }

    .link {
        padding: 12px;
        text-align: center;
    }

    .a {
        margin:auto;
        max-width: 300px;
        display: block;
        background: #69bc1f;
        color: white;
        padding: 12px;
        border-radius: 4px;
        text-align: center;
        color: white;
        text-decoration: none;
    }
    
    .company {
        padding: .4em;
        font-size: 20px;
        font-weight: 500;
        text-decoration: none;
        color: rgba(0, 0, 0, 0.54);
    }

    .logo {
        height: 42px;
        padding-top: 8px;
    }

    .header {
        height: 64px;
        line-height: 32px;
        background-color:white;
        color: rgba(0, 0, 0, 0.54);
        display: flex;
        box-sizing: border-box;
        padding: 0 16px;
        width: 100%;
        flex-direction: row;
        align-items: center;
        white-space: nowrap;
    }
